.tk-font-here
	font-family $sans
	
body
	background white

.test canvas 
	display block
	background #efefef
	margin 5em auto
	
.flock2
	margin 5em auto
	position relative
	canvas
		position absolute
		top 0
		left 0
		display block
		background #fff
		margin 0 auto
	svg 
		position absolute
		top 0
		left 0
		display block
		margin 0 auto
		font-family Helvetica
		text-transform uppercase
		font-size 12px
		font-weight bold
		fill #666
	
	.ring 
		path 
			fill none
			stroke $gray-light
			stroke-dasharray 4 4