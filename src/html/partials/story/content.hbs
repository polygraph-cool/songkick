<div id='made'>
	<div class='made__vis'>
		<div class='made__chart' id='made__pin'>
			<div class='small small-0'></div>
			<div class='small small-1'></div>
			<div class='small small-2'></div>
			<div class='small small-3'></div>
			<div class='small small-4'></div>
			<div class='small small-5'></div>
			<div class='small small-6'></div>
			<div class='small small-7'></div>
			<div class='small small-8'></div>
			<div class='small small-9'></div>
		</div>
		<div class='made__annotation tk-whitney is-hidden'>
			<p>3,180 bands that played multiple shows from 2013-2016</p>
		</div>
	</div>
	<div class='made__prose'>
		{{> story/intro }}
		{{#each copy.made}}
			<div class='trigger {{className}}' data-band='{{band}}' data-id='{{id}}'>
				{{#if band}}
				<div class='band__img' style='background-image: url("https://i.scdn.co/image/{{img}}");'></div>
				<ul class='band__list tk-whitney'>
					<li class='list__name'>{{name}} <span class='btn__audio' data-src='{{audio}}'>@@include('misc/audio-on.svg')</span></li>
					<li class='list__date'>Date: {{date}}</li>
					<li class='list__venue'>Venue: {{venue}}</li>
				</ul>
				{{else if video}}			
				 <div class='inline-video'></div>
				{{else if list}}
				<p>{{{text}}}</p>
				<ul class='band__list-others tk-whitney'>
					{{#each other}}
					<li>{{name}}</li>
					{{/each}}
				</ul>
				{{else}}
				<p>{{{text}}}</p>
				{{/if}}
			</div>
		{{/each}}
	</div>
</div>

<div class='section__header tk-whitney-ssm-a-900'>
	<h2 class='header__hed'>How long it takes to make it</h2>
</div>

<div class='bridge'>
	{{#each copy.ascent}}
	<p>{{{value}}}</p>
	{{/each}}
</div>

<div id='ascent' class='tk-whitney'>
	<div class='ascent__vis'></div>
</div>

<div class='section__header tk-whitney-ssm-a-900'>
	<h2 class='header__hed'>Everybody else</h2>
</div>

<div class='bridge'>
	{{#each copy.search}}
	<p>{{{value}}}</p>
	{{/each}}
</div>

<div id='search' class='tk-whitney'>
	<ul class='search__vis'></ul>
	<div class='search__popup popup'>
		<p class='popup__name'></p>
		<div class='popup__info'>
			<div class='left'>
				<p class='popup__text'></p>
				<div class='popup__vis'><svg></svg></div>
			</div>
			<div class='right'>
				<p class='popup__recent'>Recent shows</p>
				<ul class='popup__shows'></ul>
			</div>
		</div>
	</div>

	<div class='search__find'>
		<div class='find__container'>
			<div class='find__input'>
				<input placeholder='Find an artist or band' type='text' class='tk-whitney'>
				<div class='find__icon'>
					@@include('misc/search.svg')
				</div>
			</div>
		</div>
		<ul class='find__results'></ul>
		<div class='find__popup popup'>
			<div class='popup__close'>
				@@include('misc/close.svg')
			</div>
			<p class='popup__name'></p>
			<div class='popup__info'>
				<div class='left'>
					<p class='popup__text'></p>
					<div class='popup__vis'><svg></svg></div>
				</div>
				<div class='right'>
					<p class='popup__recent'>Recent shows</p>
					<ul class='popup__shows'></ul>
				</div>
			</div>
		</div>
		
	</div>
</div>

